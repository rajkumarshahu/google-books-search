  "bugs": {
    "url": "https://github.com/rajkumarshahu/google-books-search/issues"
  },
  "homepage": "https://github.com/rajkumarshahu/google-books-search#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.12",
    "nodemon": "^1.19.2",
    "react-alertify-js": "0.0.1"
  }
}
